+static int get_vendor_id(struct android_dev *dev)
+{
+       struct android_usb_product *p = dev->products;
+       int count = dev->num_products;
+       int i;
+
+       if (p) {
+               for (i = 0; i < count; i++, p++) {
+                       if (p->vendor_id && product_matches_functions(p))
+                               return p->vendor_id;
+               }
+       }
+       /* use default vendor ID */
+       return dev->vendor_id;
+}
+
